<div class="box" style="padding:0 !important;border-radius:5px">
    <div class="portfolio-wrap dashboard-service-image">
      {% if publicity.filename %}
        {% if publicity.link %}
              <a  href="{{publicity.link}}" target="_blank" >
                <img src="{{ vich_uploader_asset(publicity, 'imageFile') }}" class="img-fluid" alt="" style="border-radius:5px">
              </a>
        {% else %}
             <img src="{{ vich_uploader_asset(publicity, 'imageFile') }}" class="img-fluid" alt="" style="border-radius:5px">
        {% endif %}
      {% endif %}
    </div>
</div>